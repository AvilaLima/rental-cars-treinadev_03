<h1>Modelos de Carros </h1>
<table>
    <thead>
    <tr>
        <th>Fabricante</th>
        <th>Nome</th>
        <th>Ano</th>
    </tr>
    </thead>
    <tbody>
    <% @car_models.each do |cm| %>
    <tr>
        <td><%= cm.manufacturer.name %></td>
        <td><%= cm.name %></td>
        <td><%= cm.year %></td>
        <td><%=  link_to 'Ver Detalhes', cm %></td>
    </tr>
    <% end %>
    </tbody>
</table>